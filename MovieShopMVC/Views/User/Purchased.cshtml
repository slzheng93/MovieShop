@model ApplicationCore.Models.PurchaseResponseModel

<div class="rounded">
    <div class="container-fluid">
        <h1 class="text-md-center">!!! Movies You Have Purchased !!!</h1>
        <div class="row">
            @foreach (var movie in Model.PurchasedMovies)
            {
                <div class="col-6 col-xl-2 col-sm-4 col-lg-3"> 
                    <partial name="_PurchasedMovieCards" model="movie"/>
                </div>
            }
        </div>
    </div>
</div>