@model ApplicationCore.Models.FavoriteResponseModel

<div class="rounded">
    <div class="container-fluid">
        <h1 class="text-md-center">!!! Movies You Have Favorited !!!</h1>
        <div class="row">
            @foreach (var movie in Model.FavoriteMovies)
            {
                <div class="col-6 col-xl-2 col-sm-4 col-lg-3"> 
                    <partial name="_MovieCard" model="movie"/>
                </div>
            }
        </div>
    </div>
</div>